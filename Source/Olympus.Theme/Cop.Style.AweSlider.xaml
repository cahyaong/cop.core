<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:olympus="http://schemas.ngratis.com/2014/olympus">
  <ResourceDictionary.MergedDictionaries>
    <olympus:SharedResourceDictionary Source="/FirstFloor.ModernUI;component/Assets/Slider.xaml" />
  </ResourceDictionary.MergedDictionaries>
  <Style
    x:Key="{x:Type olympus:AweSlider}"
    BasedOn="{StaticResource {x:Type Slider}}"
    TargetType="olympus:AweSlider" />
</ResourceDictionary>